--TableB:findB1
select * from Table_B t
<#if fieldE?has_content && fieldE?starts_with("H")>
  where t.field_E = :fieldE
</#if>

--TableB:findB2
select * from Table_B t
<#if maxD??>
  where t.field_D < :maxD
</#if>

--TableB:sumB1
select sum(FIELD_D) from TABLE_B
<#if maxD??>
  where FIELD_D < :maxD
</#if>